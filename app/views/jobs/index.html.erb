All Jobs for <strong><%= @company.name %></strong>
<%= link_to 'Add New Job', new_company_job_path(@company) %>
<% @jobs.each do |job| %>
  <ul>
    <%= job.title %> at <%= job.company.name %> - <%= link_to "Edit", edit_company_job_path(@company, job) %> - <%= link_to 'Delete', company_job_path(@company, job), method: :delete, data: {confirm: 'Really delete job?'} %>
  </ul>
<% end %>
